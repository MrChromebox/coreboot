CONFIG_LOCALVERSION="MrChromebox-4.9"
CONFIG_USE_BLOBS=y
CONFIG_VENDOR_GOOGLE=y
CONFIG_FSP_FILE="blobs/mainboard/google/sentry/fsp.bin"
CONFIG_CBFS_SIZE=0x450000
